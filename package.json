{
  "name": "webpack-learning",
  "version": "1.0.0",
  "description": "For learning webpack concepts and its functioning",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:prod": "npm run build -- --env appEnv=prod",
    "start": "webpack serve --config webpack/webpack.dev.js",
    "start:dev": "npm start -- --env appEnv=dev",
    "build:stats": "npm run build --profile --json > stats.json",
    "build:analyze": "npm run build:prod analyze"
  },
  "keywords": [
    "webpack",
    "learning"
  ],
  "author": "Sandeep Garg",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "purgecss-webpack-plugin": "^7.0.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
